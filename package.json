{
  "name": "controls",
  "version": "0.3.0",
  "description": "",
  "main": "dist/platypusui.js",
  "devDependencies": {
    "glob": "latest",
    "grunt": "latest",
    "grunt-cli": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-copy": "latest",
    "grunt-contrib-cssmin": "latest",
    "grunt-contrib-less": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-contrib-watch": "latest",
    "grunt-less-bundle": "latest",
    "grunt-ts": "latest",
    "grunt-ts-bundle": "latest",
    "http-server": "latest",
    "load-grunt-tasks": "latest",
    "lodash": "latest",
    "platypus": "latest",
    "rimraf": "latest",
    "tsd": "^0.6.0-beta.5",
    "typescript": "latest"
  },
  "scripts": {
    "prepublish": "npm run deploy",
    "build-all": "npm run build-examples && npm run build-src",
    "build-examples": "node_modules/.bin/tsc -p build/tsc/examples",
    "build-src": "node_modules/.bin/tsc -p build/tsc/src",
    "clean-all": "npm run clean-examples && npm run clean-src",
    "clean-examples": "node_modules/.bin/rimraf -p examples/app.*",
    "clean-src": "node_modules/.bin/rimraf -p dist/*",
    "deploy": "node_modules/.bin/grunt",
    "examples": "npm run less-examples && npm run build-examples && npm run server-examples",
    "less-examples": "node_modules/.bin/grunt lessCompile:examples",
    "server-examples": "node_modules/.bin/http-server -p 3000",
    "watch-examples": "node_modules/.bin/tsc -p build/tsc/examples/watch",
    "watch-src": "node_modules/.bin/tsc -p build/tsc/src/watch"
  },
  "author": "",
  "license": "GPL-3.0"
}
